{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.svg",
      "/*.ico",
      "/*.woff",
      "/*.woff2",
      "/*.ttf"
    ]
  },
  "globalHeaders": {
    "cache-control": "no-cache, no-store, must-revalidate"
  }
}
